{
   "action" : "add",
   "description" : "Create a service in a given GSLB Farm.",
   "method" : "POST",
   "params" : {
      "algorithm" : {
         "non_blank" : "true",
         "required" : "true",
         "values" : [
            "roundrobin",
            "prio"
         ]
      },
      "id" : {
         "exceptions" : [
            "0"
         ],
         "non_blank" : "true",
         "regex" : "(?^:[a-zA-Z0-9][\\w\\-]*)",
         "required" : "true"
      }
   },
   "url" : "/farms/<farmname>/services"
}
