{
   "action" : "set",
   "description" : "If the object has an alias, it will be replaced, else an alias will be created for the object.",
   "method" : "PUT",
   "params" : {
      "alias" : {
         "exceptions" : [
            "0"
         ],
         "non_blank" : "true",
         "regex" : "(?^:(?:(?^:(?:(?^:[a-z][a-z0-9\\-]{0,253}[a-z0-9])\\.)+[a-z]{2,})|[\\w-]+))",
         "required" : "true"
      }
   },
   "url" : "/aliases/<backends|interfaces>/<id>"
}
