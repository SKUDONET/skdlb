{
	"method": "PUT",
	"url": "/farms/<farmname>/backends/<id>",
	"action": "set",
	"description": "Modify the parameters of a backend in for a L4xNAT Farm. id is the backend unique identifier.",
	"params": {
		"priority": {
			"interval": "1,9"
		},
		"weight": {
			"interval": "1,9"
		},
		"ip": {
			"regex": "$ip_addr",
			"non_blank": "true",
			"format_msg": "expects an IP"
		},
		"port": {
			"regex": "$port"
		},
		"max_conns": {
			"interval": "0,"
		},
		"interface": {
			"non_blank": "true"
		}
	}
}
