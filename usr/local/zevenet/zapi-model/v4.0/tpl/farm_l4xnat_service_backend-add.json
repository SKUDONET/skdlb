{
	"method": "POST",
	"url": "/farms/<farmname>/backends",
	"action": "add",
	"description": "Create a new Backend in a given L4xNAT Farm.",
	"params": {
		"priority": {
			"interval": "1,9"
		},
		"weight": {
			"interval": "1,9"
		},
		"ip": {
			"regex": "$ip_addr",
			"non_blank": "true",
			"format_msg": "expects an IP",
			"required": "true"
		},
		"port": {
			"regex": "$port",
			"format_msg": "expects a port or port range"
		},
		"max_conns": {
			"interval": "0,"
		}
	}
}
