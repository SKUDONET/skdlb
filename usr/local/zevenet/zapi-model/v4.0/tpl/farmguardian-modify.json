{
	"method": "PUT",
	"url": "/monitoring/fg/<name>",
	"action": "set",
	"description": "Modify the parameters of the health check",
	"params": {
		"description": {
			"non_blank": "true"
		},
		"command": {
			"non_blank": "true"
		},
		"log": {
			"values": [
				"true",
				"false"
			],
			"non_blank": "false"
		},
		"interval": {
			"regex": "$natural_num"
		},
		"timeout": {
			"regex": "$natural_num"
		},
		"cut_conns": {
			"values": [
				"true",
				"false"
			],
			"non_blank": "false"
		},
		"backend_alias": {
			"values": [
				"true",
				"false"
			],
			"non_blank": "false"
		},
		"force": {
			"values": [
				"true",
				"false"
			],
			"non_blank": "false"
		}
	}
}
