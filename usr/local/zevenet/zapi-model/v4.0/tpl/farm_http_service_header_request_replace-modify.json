{
	"method": "PUT",
	"url": "/farms/<farmname>/service/<servicename>/replacerequestheader/<id>",
	"action": "modify",
	"description": "Add rules to rewrite headers from the request, only for this service.",
	"params": {
		"header": {
			"non_blank": "true",
			"is_regex": "true",
			"negated_regex": "$newline"
		},
		"match": {
			"non_blank": "true",
			"is_regex": "true",
			"negated_regex": "$newline"
		},
		"replace": {
			"negated_regex": "$newline"
		}
	}
}
