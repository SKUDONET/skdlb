{
	"method": "PUT",
	"url": "/farms/<farmname>/services/<id>",
	"action": "set",
	"description": "Modify the parameters of a service in an HTTP profile.",
	"params": {
		"vhost": {
			"is_regex": "true",
			"negated_regex": "$startline"
		},
		"urlp": {
			"is_regex": "true"
		},
		"redirect": {
			"non_blank": "false",
			"regex": "$url"
		},
		"redirecttype": {
			"values": [
				"default",
				"append"
			]
		},
		"leastresp": {
			"regex": "$boolean",
			"non_blank": "true"
		},
		"persistence": {
			"values": [
				"IP",
				"BASIC",
				"NONE",
				"URL",
				"PARM",
				"COOKIE",
				"HEADER"
			],
			"non_blank": "false"
		},
		"sessionid": {},
		"ttl": {
			"non_blank": "true",
			"regex": "$natural_num"
		},
		"httpsb": {
			"non_blank": "true",
			"regex": "$boolean"
		},
		"pinnedconnection": {
			"non_blank": "true",
			"values": [
				0,
				1
			]
		},
		"routingpolicy": {
			"non_blank": "true",
			"values": [
				"ROUND_ROBIN",
				"LEAST_CONNECTIONS",
				"RESPONSE_TIME",
				"PENDING_CONNECTIONS"
			]
		},
		"rewritelocation": {
			"non_blank": "true",
			"values": [
				"disabled",
				"enabled",
				"enabled-backends",
				"enabled-path",
				"enabled-backends-path"
			]
		},
		"redirect_code": {
			"edition": "ee",
			"values": [
				301,
				302,
				307
			]
		},
		"sts_timeout": {
			"edition": "ee",
			"regex": "$http_sts_timeout"
		},
		"sts_status": {
			"edition": "ee",
			"values": [
				"true",
				"false"
			]
		},
		"cookieinsert": {
			"edition": "ee",
			"values": [
				"true",
				"false"
			]
		},
		"cookiettl": {
			"edition": "ee",
			"regex": "$natural",
			"non_blank": "true"
		},
		"cookiename": {
			"edition": "ee"
		},
		"cookiedomain": {
			"edition": "ee"
		},
		"cookiepath": {
			"edition": "ee"
		}
	}
}
