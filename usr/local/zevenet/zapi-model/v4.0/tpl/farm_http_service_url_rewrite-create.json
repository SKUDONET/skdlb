{
	"method": "POST",
	"url": "/farms/<farmname>/service/<servicename>/rewriteurl",
	"action": "add",
	"description": "It checks a pattern in order to get strings from URL and replace them.",
	"params": {
		"pattern": {
			"non_blank": "true",
			"required": "true",
			"is_regex": "true",
			"negated_regex": "$newline"
		},
		"replace": {
			"required": "true",
			"negated_regex": "$newline"
		},
		"last": {
			"non_blank": "true",
			"required": "true",
			"values": [
				"true",
				"false"
			]
		}
	}
}
