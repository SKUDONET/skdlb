{
	"method": "PUT",
	"url": "/farm/<farmname>",
	"action": "set",
	"description": "Modify the configuration of a datalink farm.",
	"params": {
		"newfarmname": {
			"regex": "$farm_name",
			"non_blank": "true",
			"exceptions": [
				"0"
			]
		},
		"algorithm": {
			"values": [
				"prio",
				"weight"
			],
			"non_blank": "true"
		},
		"vip": {
			"dyn_values": "true",
			"non_blank": "true",
			"format_msg": "expects an IP"
		}
	}
}
