{
	"method": "POST",
	"url": "/ipds/waf/<name>/rules/<id>/matches",
	"action": "add",
	"description": "A matching rule extends a WAF action rule, adding match conditions.",
	"params": {
		"not_match": {
			"regex": "$boolean"
		},
		"variables": {
			"non_blank": "true",
			"ref": "array",
			"required": "true"
		},
		"operator": {
			"non_blank": "true",
			"dyn_values": "true"
		},
		"operating": {},
		"transformations": {
			"ref": "array"
		},
		"multi_match": {
			"regex": "$boolean"
		},
		"capture": {
			"regex": "$boolean"
		}
	}
}
