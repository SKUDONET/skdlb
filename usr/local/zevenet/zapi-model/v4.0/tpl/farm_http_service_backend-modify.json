{
	"method": "PUT",
	"url": "/farms/<farmname>/services/<id>/backends/<id>",
	"action": "set",
	"description": "Modify the parameters of a backend HTTP service.",
	"params": {
		"weight": {
			"interval": "1,9"
		},
		"timeout": {
			"regex": "$natural_num"
		},
		"ip": {
			"regex": "$ip_addr",
			"non_blank": "true",
			"format_msg": "expects an IP"
		},
		"port": {
			"regex": "$port",
			"non_blank": "true"
		},
		"priority": {
			"depend_on": "proxy_ng==true",
			"interval": "1,9"
		},
		"connection_limit": {
			"depend_on": "proxy_ng==true",
			"interval": "0,"
		}
	}
}
