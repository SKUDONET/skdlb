{
	"method": "PUT",
	"url": "/farms/<farmname>/actions",
	"action": "status",
	"description": "Apply an action to a farm, see the Request parameters table for actions.",
	"params": {
		"action": {
			"values": [
				"stop",
				"start",
				"restart"
			],
			"non_blank": "true",
			"required": "true"
		}
	}
}
