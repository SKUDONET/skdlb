{
	"method": "POST",
	"url": "/farms/<farmname>/services/<id>/backends",
	"action": "add",
	"description": "Create a new Backend in a given HTTP profile. id is the service unique identifier.",
	"params": {
		"weight": {
			"interval": "1,9"
		},
		"timeout": {
			"regex": "$natural_num"
		},
		"ip": {
			"regex": "$ip_addr",
			"non_blank": "true",
			"format_msg": "expects an IP",
			"required": "true"
		},
		"port": {
			"regex": "$port",
			"non_blank": "true",
			"required": "true"
		},
		"connection_limit": {
			"depend_on": "proxy_ng==true",
			"interval": "0,"
		},
		"priority": {
			"depend_on": "proxy_ng==true",
			"interval": "1,9"
		}
	}
}
