{
	"method": "POST",
	"url": "/farms/<farmname>/services/<servicename>/sessions",
	"action": "add",
	"description": "It creates a static session with a backend in a http farm.",
	"params": {
		"id": {
			"non_blank": "true",
			"required": "true",
			"regex": "$integer",
			"dyn_values": "true"
		},
		"session": {
			"non_blank": "true",
			"required": "true"
		}
	}
}
