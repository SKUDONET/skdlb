{
	"method": "POST",
	"url": "/aws/credentials",
	"action": "set",
	"description": "Set IAM user credentials.",
	"params": {
		"access_key": {
			"description": "Access key of IAM user.",
			"non_blank": "true"
		},
		"secret_key": {
			"description": "Secret key of IAM user.",
			"non_blank": "true"
		},
		"region": {
			"description": "IAM User hosted location.",
			"non_blank": "true"
		}
	}
}
