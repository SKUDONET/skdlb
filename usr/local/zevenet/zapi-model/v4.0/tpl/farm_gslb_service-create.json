{
	"method": "POST",
	"url": "/farms/<farmname>/services",
	"action": "add",
	"description": "Create a service in a given GSLB Farm.",
	"params": {
		"id": {
			"regex": "$gslb_service",
			"non_blank": "true",
			"required": "true",
			"exceptions": [
				"0"
			]
		},
		"algorithm": {
			"values": [
				"roundrobin",
				"prio"
			],
			"non_blank": "true",
			"required": "true"
		}
	}
}
