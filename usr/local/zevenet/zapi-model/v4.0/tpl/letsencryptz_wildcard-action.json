{
	"method": "POST",
	"url": "/certificates/letsencryptz-wildcard/<domain>/actions",
	"action": "renew",
	"description": "Apply an action in a wildcard Letsencrypt certificate by first domain.",
	"params": {
		"action": {
			"values": [
				"renew",
				"test"
			],
			"non_blank": "true",
			"required": "true"
		},
		"force_renewal": {
			"values": [
				"true",
				"false"
			]
		},
		"restart": {
			"values": [
				"true",
				"false"
			]
		},
		"dns": {
			"regex": "$dns_nameserver"
		}
	}
}
